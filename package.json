{
  "name": "next-spa-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "404": "cp out/404/index.html out/404.html",
    "test": "jest --passWithNoTests",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "analyze": "ANALYZE=true next build",
    "prettier:check": "prettier --check src",
    "prettier:fix": "prettier --write src",
    "elint:check": "next lint --ext .ts,.js,.tsx,.jsx src",
    "elint:fix": "next lint --fix --ext .ts,.js,.tsx,.jsx src",
    "lint": "pnpm run --parallel /^.*:check$/",
    "lint-fix": "pnpm run /^.*:fix$/",
    "clean": "rimraf build",
    "pretest": "pnpm build",
    "all": "pnpm install && pnpm lint && pnpm test",
    "postbuild": "pnpm 404",
    "serve": "http-server out -p 8080"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.19",
    "@mui/material": "^5.14.20",
    "i18next": "^23.7.7",
    "next": "^14.0.3",
    "next-i18next": "^15.0.0",
    "next-language-detector": "^1.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "~13.5.0",
    "web-vitals": "^3.5.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.0.3",
    "@types/node": "^20.10.3",
    "@types/react": "^18.2.42",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-config-import": "^0.13.0",
    "eslint-config-next": "^14.0.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "http-server": "^14.1.1",
    "next-compose-plugins": "^2.2.1",
    "next-transpile-modules": "^10.0.1",
    "postcss": "^8.4.32",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.2"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    }
  }
}
