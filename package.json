{
  "name": "next-spa-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "404": "cp out/404/index.html out/404.html",
    "test": "jest --passWithNoTests",
    "dev": "next dev",
    "build": "next build && next export",
    "start": "next start",
    "analyze": "ANALYZE=true next build",
    "elint-check": "next lint --ext .ts,.js,.tsx,.jsx src",
    "elint-fix": "next lint --fix --ext .ts,.js,.tsx,.jsx src",
    "prettier-check": "prettier --check src",
    "prettier-format": "prettier --write src",
    "lint": "yarn prettier-check && yarn elint-check",
    "lint-fix": "yarn prettier-format && yarn elint-fix",
    "clean": "rimraf build",
    "pretest": "yarn build",
    "all": "yarn install && yarn lint && yarn test",
    "postbuild": "yarn 404",
    "serve": "http-server out -p 8080"
  },
  "dependencies": {
    "@emotion/react": "*",
    "@emotion/server": "*",
    "@emotion/styled": "*",
    "@mui/icons-material": "*",
    "@mui/material": "*",
    "i18next": "*",
    "next": "^13",
    "next-i18next": "*",
    "next-language-detector": "*",
    "react": "*",
    "react-dom": "*",
    "react-i18next": "~12.0",
    "web-vitals": "*"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13",
    "@next/font": "^13",
    "@swc/jest": "*",
    "@types/jest": "*",
    "@types/node": "*",
    "@types/react": "*",
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "autoprefixer": "*",
    "eslint": "*",
    "eslint-config-airbnb-base": "*",
    "eslint-config-airbnb-typescript": "*",
    "eslint-config-import": "*",
    "eslint-config-next": "^13",
    "eslint-config-prettier": "*",
    "eslint-import-resolver-typescript": "*",
    "eslint-plugin-flowtype": "*",
    "eslint-plugin-import": "*",
    "eslint-plugin-jest": "*",
    "eslint-plugin-jsx-a11y": "*",
    "eslint-plugin-prettier": "*",
    "eslint-plugin-react": "*",
    "eslint-plugin-react-hooks": "*",
    "http-server": "*",
    "jest": "*",
    "next-compose-plugins": "*",
    "next-transpile-modules": "*",
    "postcss": "*",
    "prettier": "*",
    "rimraf": "*",
    "tailwindcss": "*",
    "typescript": "*"
  },
  "resolutions": {
    "json5": ">=2.2.2"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    }
  }
}
